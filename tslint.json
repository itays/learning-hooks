{
	"defaultSeverity": "error",
	"extends": ["tslint:recommended"],
	"jsRules": {},
	"rules": {
		"max-line-length": {
			"options": [120]
		},
		"quotemark": [true, "single", "jsx-double"],
		"ordered-imports": false,
		"indent": [true, "tabs", 2],
		"member-access": false,
		"object-literal-sort-keys": false,
		"trailing-comma": false
	},
	"rulesDirectory": [],
	"linterOptions": {
		"exclude": ["./src/serviceWorker.ts"]
	}
}
